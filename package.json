{
  "name": "carto-api-client",
  "version": "0.0.10",
  "description": "Client for the Builder backend API",
  "main": "dist/carto-api-client.js",
  "scripts": {
    "build": "webpack --env dev && webpack --env build",
    "bump": "npm version patch",
    "dev": "webpack --progress --colors --watch --env dev",
    "test:build": "webpack --config webpack.test.config.js --watch",
    "test:server": "webpack-dev-server --config webpack.test.config.js --hot --inline --open-page tests",
    "docs": "rm -rf docs/public; jsdoc --configure config/jsdoc/config.json"
  },
  "author": {
    "name": "CARTO",
    "url": "http://carto.com/",
    "email": "wadus@carto.com"
  },
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "https://github.com/cartodb/carto-api-client.git"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "eslint": "^4.9.0",
    "eslint-config-semistandard": "^11.0.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-chai-friendly": "^0.4.0",
    "eslint-plugin-destructuring": "^2.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "jsdoc-babel": "^0.3.0",
    "jsdoc-webpack-plugin-v2": "^2.0.2",
    "mocha": "^4.0.1",
    "mocha-loader": "^1.1.1",
    "mocha-webpack": "^0.7.0",
    "sinon": "^4.0.1",
    "sinon-chai": "^2.14.0",
    "webpack": "^3.8.1",
    "webpack-dev-server": "^3.1.10",
    "yargs": "^9.0.1"
  },
  "dependencies": {
    "whatwg-fetch": "^2.0.3"
  }
}
